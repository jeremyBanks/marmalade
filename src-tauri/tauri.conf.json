{
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "devPath": "https://jam.gg/play",
    "distDir": "https://jam.gg/play",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "marmalade",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "marmalade.jeremy.ca",
      "targets": ["appimage"]
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "Marmalade",
        "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/111.0.0.0 Safari/537.36",
        "fullscreen": true,
        "resizable": true,
        "width": 1280,
        "height": 800
      }
    ]
  }
}
