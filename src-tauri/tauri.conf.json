{
  "build": {
    "devPath": "https://jam.gg/play",
    "distDir": "https://jam.gg/play",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "marmalade",
    "version": "0.1.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "marmalade.jeremy.ca",
      "targets": ["appimage"]
    },
    "windows": [
      {
        "fullscreen": true,
        "resizable": true,
        "title": "Jam via Marmalade",
        "width": 1280,
        "height": 800
      }
    ]
  }
}
